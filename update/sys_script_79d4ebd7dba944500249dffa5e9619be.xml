        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var gr = new GlideRecord('x_gege9_asset_scan_todo');
	gr.addQuery('asset', current.sys_id.toString());
	gr.addQuery('type', 500);
	gr.query();
	while(gr.next()){
		gr.state = 7;
		gr.update();
	}

})(current, previous);]]></script>
